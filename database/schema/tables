active_events:
	serial			bigint		(Serial)
	dedup_id		text		(Identifier)
	event_id		text		(Fault Resolution Identifier)
	type			int 		(Type)
	corr_id			text		(AlertGroup)
	source			text		(Manager)
	source_type		text		(Agent)
	node 			text		(Node)
	object			text		(AlertKey)
	message			text		(Summary)
	start			datetime	(FirstOccurrence)
	last_occurrence	datetime	(LastOccurrence)
	last_change		datetime	(LastUpdate)
	severity		int 		(Severity)
	count			int 		(Count)
	start_severity	int 		(OriginalSeverity)
	detail			json		(Description)
	ticket			text		(Ticket)
	owner_uid		text		(OwnerUID)
	group_uid		text		(GroupUID)
	supression		int 		(-)
	corr_flag		int 		(Impact Flag)
	max_life		int 		(ExpireTime)
	clear_time		datetime	(-)
	delete_time		datetime	(-)
	trace_dedup		boolean		(-)
	parent			bigint		(-)
	has_childs		boolean		(-)

history_events:
	serial			bigint		(Serial)
	dedup_id		text		(Identifier)
	event_id		text		(Fault Resolution Identifier)
	type			int 		(Type)
	corr_id			text		(AlertGroup)
	source			text		(Manager)
	source_type		text		(Agent)
	node 			text		(Node)
	object			text		(AlertKey)
	message			text		(Summary)
	start			datetime	(FirstOccurrence)
	last_occurrence	datetime	(LastOccurrence)
	last_change		datetime	(LastUpdate)
	severity		int 		(Severity)
	count			int 		(Count)
	start_severity	int 		(OriginalSeverity)
	detail			json		(Description)
	ticket			text		(Ticket)
	owner_uid		text		(OwnerUID)
	group_uid		text		(GroupUID)
	supression		int 		(-)
	corr_flag		int 		(Impact Flag)
	max_life		int 		(ExpireTime)
	clear_time		datetime	(-)
	delete_time		datetime	(-)
	trace_dedup		boolean		(Trace all deduplication changes)
	parent			bigint		(-)
	has_childs		boolean		(-)

deduplication_trace:
	serial			bigint		(Serial)
	dedup_id		text		(Identifier)
	event_id		text		(Fault Resolution Identifier)
	type			int 		(Type)
	source			text		(Manager)
	node 			text		(Node)
	object			text		(AlertKey)
	message			text		(Summary)
	occurrence		datetime	(-)
	severity		int 		(Severity)
	extra_details	text		(-)

